<!-- Modal to change password -->
<div class="modal fade" id="modal-password-change" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display:none;">
	<div class="modal-dialog">
		<div class="modal-content">
