<!-- Modal to Add Inputs -->
<div class="modal fade medium-modal-dialog" id="modal-inputs-add" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display:none;">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title" id="inputs-add-label">Add Input</h4>
			</div>
			<div class="modal-body" id="modal-body-inputs-add">
			<a target="_blank" tabindex="-1" href=<%= @input_link %> >How to set input?</a>
				<div class="textbox">
					<div class="alert alert-danger" role="alert" style="display:none;" id="alert-modal-setup-inputs-add-confirm"></div>
					<strong class="strong-text">Input Type</strong><br><br>
					<form class="input-radio-form-inline" name="radioform">
						<label class="radio-inline">
							<input type="radio" name="InputRadio" id = "add-input-log-radio" onchange="addInputSwithButton()" > <i class="fa fa-file-o no-margin-icon" aria-hidden="true"></i>Log File Input
						</label>
						<label class="radio-inline">
							<input type="radio" name="InputRadio" id = "add-input-script-radio" onchange="addInputSwithButton()" > <i class="fa fa-code no-margin-icon" aria-hidden="true"></i>Script Input
						</label>
						<label class="radio-inline">
							<input type="radio" name="InputRadio" id = "add-input-unixapp-radio" onchange="addInputSwithButton()" > <i class="fa fa-linux no-margin-icon" aria-hidden="true"></i>Unix App Input
						</label>
					</form>

					<br>
					<%= form_tag '/splunk_users/setupinputconfirm', :id => "add-input-form", :class => 'form-horizontal', :role => "form" do -%>
					
                  	<hr class="hr-line">
					<strong class="strong-text"> Basic Settings</strong><br><br>

					<div id="add-input-log-basic-div">
					<!--log file path-->
						<label for="source-hostname" class="modal-label control-label">Log File Path</label>
						<div class="form-group">
						 <%= text_area_tag 'log_file_path','' ,id: 'input-setup-inputs-add-log-file-path', placeholder: 'Log File Path', data: { toggle: 'popover', trigger: 'hover', content: '' }, :autocomplete => :off , :rows => '3', :class => 'modal-input' %>*
						</div>
					</div>

					<div id="add-input-unix-basic-div">
						<p class="msg-info"><i class="fa fa-info-circle" aria-hidden="true"></i>Unix App Inputs are prewritten shell scripts, they can only be executed on *nix environments</p>
						<p class="msg-info"><i class="fa fa-info-circle" aria-hidden="true"></i>The more frequently the script is executed, the larger the input size will be</p>
						<p class="msg-info"><i class="fa fa-info-circle" aria-hidden="true"></i>The most commonly used scripts are: cpu, df, vmstat, iostat and interfaces</p>
						Check source code of each script from <a class="fixe-link" target="_blank" tabindex="-1" href=<%= @unixapp_link%> >HERE</a><br><br>
						<button type="button" class = "btn btn-info" onclick="setRecommendedUnixAppInput()">Recommended Configurations</button>
						<button type="button" class = "btn btn-info" onclick="resetUnixAppInput()">Reset</button>
						<input type="text" id="" class="search-field textbox" placeholder = "Search Filter" onkeyup="doSearch(this, 'modal-add-unixapp-input-table')"></input><br><br>
						<table id="modal-add-unixapp-input-table" class="table table-bordered table-hover">
						</table>
					</div>

					<!-- common -->
					<!--srouce type-->
					<div id="add-input-non-app-common">
						<label for="source-hostname" class="modal-label control-label">Source Type</label>
						<div class="form-group">
							<%= text_area_tag 'sourcetype','' ,id: 'input-setup-inputs-add-source-type', placeholder: 'Sourcetype', data: { toggle: 'popover', trigger: 'hover', content: '' }, :autocomplete => :off, :rows => '1', :class => 'modal-input' %>*
						</div>

						 <!--Log File Size Per Day-->
						<label for="source-hostname" class="modal-label control-label">Log Size Per Day</label>
						<div class="form-group">
							<%= select_tag(:log_file_size, options_for_select([['< 100 MB'], ['< 500 MB'], ['< 1000 MB'], ['>1000 MB']], selected: :option ), id: 'select-setup-inputs-add-log-file-size',:class => 'modal-input') %>*
						</div>

						 <!--Data Retention Period-->
						<label  for="source-hostname" class="modal-label control-label">Data Retention Period</label>
						<div class="form-group">
							<%= select_tag(:data_retention_period, options_for_select([['1 Day'], ['2 Day'], ['3 Day'], ['1 Week'], ['2 Week'], ['3 Week'], ['1 Month'], ['3 Month'],  ['6 Month'], ['1 Year'], ['3 Year'], ['6 Year']], selected: :option ), id: 'select-setup-inputs-add-log-retention-period', :class => 'modal-input') %>*
						</div>
					</div>


					<div id="add-input-script-basic-div">
						<label  for="source-hostname" class="modal-label control-label">Script Runs On</label>
						<div class="form-group">
							<form name="osform" action="">
								<input type="radio" name="OSRadio" id = "add-input-nix-radio" style = "margin:-2px 5px 0 0;">*nix
								<input type="radio" name="OSRadio" id = "add-input-win-radio" style = "margin:-2px 5px 0 20px;" >Windows
							</form>
						</div>

						<!--interval-->
						<p class="msg-info"><i class="fa fa-info-circle" aria-hidden="true"></i>The more frequently the script is executed, the larger the input size will be</p>
						<label  for="source-hostname" class="modal-label control-label">Interval</label>
						<div class="form-group">
							<%= text_area_tag 'interval','' ,id: 'input-setup-inputs-script-add-interval', placeholder: '<number>or<cron schedule>', data: { toggle: 'popover', trigger: 'hover', content: '' }, :autocomplete => :off , :rows => '1', :class => 'modal-input' %>*
						</div>

						 <!--Script File-->
						<label  for="source-hostname" class="modal-label control-label">Script File</label>
						<div class="form-group">
							<%= file_field_tag 'script', id: 'input-setup-inputs-script-add-script', :class => 'modal-input' %>
						</div>
						<p class="msg-info"><i class="fa fa-info-circle" aria-hidden="true"></i>Please DONOT upload script that contains Japanese characters</p>

					</div> <!-- end of <div id="add-input-script-basic-div"> -->

                  	<hr class="hr-line">
					<strong class="strong-text">Advanced Settings</strong><br><br>

					<div id="add-input-log-advanced-div">
							<!--crcSalt-->
						<label for="source-hostname" class="modal-label control-label" >crcSalt</label>
						<div class="form-group add-input-srcsalt">
						  <label class="btn add-crcsalt-label"><input id = "select-setup-inputs-add-log-crcsalt" type="checkbox" value="">&lt;SOURCE&gt;</label> 
						</div>

						<!--BlackList Files-->
						<label  for="input-blacklist" class="modal-label control-label">BlackList Files</label>
						<button type="button" class = "btn btn-info" id = "add-input-blacklist-switch-button" onclick="addInputBlacklistSwithButton()">Click to write your blacklist regex</button>

						<div class="form-group">
							<div class="add-input-blacklist" id="add-input-blacklist-checkbox">
								<% if @blacklists.present? %>
									<% @blacklists.each do |bk| %>
										<label class="btn"><input type="checkbox" value=<%= bk %> ><%= bk %></label> 
									<% end %>
								<% end %>
							</div>
							<div id="add-input-blacklist-customize">
								<ol>
									<li>
										<p>To ignore and not monitor only files with the .gz extension.</p>
										<%= text_area_tag 'example','\.(gz)$', :rows => '1', disabled: true %>
									</li>
									<li>
										<p>To igonore multiple files, e.g: ingore .gz and .txt files.</p>
										<%= text_area_tag 'example','\.(gz|txt)$', :rows => '1', disabled: true %>
									</li>

									<li>
										<p>To ignore all files under log file path within the archive or historical directories and all files ending in *.bak.</p>
										<%= text_area_tag 'example','(archive|historical|\.bak$)', :rows => '1', disabled: true %>
									</li>

									<li>
										<p>To ignore files whose names contain a specific string, e.g: ignore the webserver20090228file.txt and webserver20090229file.txt files.</p>
										<%= text_area_tag 'example','2009022[8|9]file\.txt$', :rows => '1',disabled: true %>
									</li>
								</ol>

								<strong class="strong-text">Note: If you write your own regex here, the checkbox will not take effect.</strong>
								<br><br>
								<%= text_area_tag 'blacklist_files','' ,id: 'input-setup-inputs-add-blacklist-files', placeholder: 'Blacklist regex', data: { toggle: 'popover', trigger: 'hover', content: '' }, :autocomplete => :off , :class => 'modal-input'%>
								<br>
							</div>
							<br>
						</div> <!-- end of <div class="form-group"> -->
					</div> <!--end of <div id="add-input-log-advanced-div"> -->

					<div id="add-input-script-advanced-div">
						<label  for="source-hostname" class="modal-label control-label">Option</label>
						<div class="form-group">
							<%= text_area_tag 'option','' ,id: 'input-setup-inputs-script-add-option', placeholder: 'option', data: { toggle: 'popover', trigger: 'hover', content: '' }, :autocomplete => :off , :rows => '1', :class => 'input-option'%>
						</div>
					</div>
					<!--Other Specifications-->
					<label  for="source-hostname" class="modal-label control-label">Other Specifications</label>
					<div class="form-group">
						<%= text_area_tag 'memo','' ,id: 'input-setup-inputs-add-memo', placeholder: 'If you have other specifications that need admin to do for you, write them here', data: { toggle: 'popover', trigger: 'hover', content: '' } , :autocomplete => :off, :rows => '3', :class => 'input-specification'%>
					</div>

					<!-- env -->
					<input type="hidden" name="env" id="hidden-text-env" value=<%= @env%>> </input>
					<% end -%>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal" >Cancel</button>
				<button type="button" class = "btn btn-primary" id = "submit-setup-inputs-add" onclick="setupNewInput(this)" style = '' >Submit</button>
			</div>
		</div>
	</div>
</div>

